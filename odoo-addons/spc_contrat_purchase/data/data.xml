<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">

       

        <record id="categorie_adherant" model="categorie.purchase.agreement">
            <field name="type_conventionne">physique</field>
            <field name="name">Adhérent</field>
           
        </record>
        
        <record id="categorie_pharmacie" model="categorie.purchase.agreement">
            <field name="type_conventionne">liberale</field>
            <field name="name">Pharmacie</field>
        </record>

         
        
         <record id="categorie_laboratoire" model="categorie.purchase.agreement">
            <field name="type_conventionne">liberale</field>
            <field name="name">Laboratoire d'analyse</field>
        </record>
        
        <record id="categorie_centre_radiologie" model="categorie.purchase.agreement">
            <field name="type_conventionne">liberale</field>
            <field name="name">Centre de radiologie</field>
        </record>
        
        
        <record id="categorie_medecin" model="categorie.purchase.agreement">
            <field name="type_conventionne">liberale</field>
            <field name="name">Médecin</field>
        </record>
        
        
        <record id="categorie_hopital" model="categorie.purchase.agreement">
            <field name="type_conventionne">morale</field>
            <field name="name">Hôpital</field>
        </record>
        
        <record id="categorie_clinique" model="categorie.purchase.agreement">
            <field name="type_conventionne">morale</field>
            <field name="name">Clinique</field>
        </record>
        
        
         <record id="vacation_product" model="product.product">
            <field name="name">Vacation</field>
            <field name="list_price">0.0</field>
            <field name="standard_price">0.0</field>
            <field name="type">service</field>
        </record>
      
      
       <record id="seq_convention_adh" model="ir.sequence">
            <field name="name">Convention Adhérent</field>
            <field name="code">purchase.agreement.adherant</field>
            <field name="prefix">CVA/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
        
        <record id="seq_convention_lib" model="ir.sequence">
            <field name="name">Convention Libérale</field>
            <field name="code">purchase.agreement.liberale</field>
            <field name="prefix">CVL/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
        
        <record id="seq_convention_morale" model="ir.sequence">
            <field name="name">Convention Morale</field>
            <field name="code">purchase.agreement.morale</field>
            <field name="prefix">CVM/%(range_year)s/</field>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field eval="True" name="use_date_range"/>
            <field eval="False" name="company_id"/>
            <field name="padding">5</field>
        </record>
      
      
    </data>
</odoo>
